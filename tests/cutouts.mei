<mei xmlns="http://www.music-encoding.org/ns/mei" meiversion="4.0.0">
  <meiHead>
    <fileDesc>
      <titleStmt>
        <title />
      </titleStmt>
      <pubStmt>
        <respStmt>
          <persName>Klaus Rettinghaus</persName>
        </respStmt>
      </pubStmt>
      <seriesStmt>
        <title>MEILER test files</title>
      </seriesStmt>
    </fileDesc>
  </meiHead>
  <music>
    <body>
      <mdiv>
        <score>
          <scoreDef meter.count="3" meter.unit="4">
            <staffGrp>
              <staffDef n="1" clef.line="2" clef.shape="G" lines="5" />
              <staffDef n="2" clef.line="4" clef.shape="F" lines="5" />
            </staffGrp>
          </scoreDef>
          <section>
            <measure n="1">
              <staff n="1">
                <layer>
                  <rest dur="4" />
                  <space dur="4" cutout="cutout" />
                  <rest dur="8" />
                  <space dur="8" cutout="cutout" />
                </layer>
              </staff>
              <staff n="2">
                <layer>
                  <space dur="4" cutout="cutout" />
                  <rest dur="4" />
                  <space dur="8" cutout="cutout" />
                  <rest dur="8" />
                </layer>
              </staff>
            </measure>
            <measure n="2" right="end" metcon="false">
              <staff n="1">
                <layer>
                  <rest dur="4" fermata="above" />
                </layer>
              </staff>
              <staff n="2">
                <layer>
                  <rest dur="4" fermata="below" />
                </layer>
              </staff>
            </measure>
          </section>
        </score>
      </mdiv>
    </body>
  </music>
